<% if(typeof errors!= 'undefined') { %>
    <% errors.forEach(error => { %>
        <p> <%= error.msg %> </p>
    <% }) %>
<% } %>

<% if(success_msg!= '') { %>
    <p><%=success_msg.toString() %> </p>
<% } %>

<% if(error_msg != '') { %>
    <%= error_msg %> 
<% } %> 
<% if(error != '') { %>
    <%= error %>
<% } %>